{
  "version": 2,
  "configurePresets": [
    {
      "name": "iree_llvm_libc_hdrgen",
      "description": "for target `libc-hdrgen`, check `libc/docs/full_cross_build.rst` ",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/../build_libc_hdrgen",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/../build_libc_hdrgen/install",
        "CMAKE_EXPORT_COMPILE_COMMANDS": true,
        "CMAKE_C_COMPILER": "/usr/bin/clang",
        "CMAKE_CXX_COMPILER": "/usr/bin/clang++",
        "CMAKE_INSTALL_RPATH_USE_LINK_PATH": true,
        "LLVM_ENABLE_ASSERTIONS": true,
        "LLVM_ENABLE_PROJECTS": "libc",
        "LLVM_TARGETS_TO_BUILD": "host;RISCV;AArch64",
        "LLVM_LIBC_FULL_BUILD": true,
        "LLVM_INCLUDE_DOCS": false,
        "LLVM_INCLUDE_EXAMPLES": false,
        "LLVM_INCLUDE_TESTS": false,
        "LLVM_INCLUDE_UTILS": false,
        "LLVM_INCLUDE_TOOLS": false,
        "LLVM_INCLUDE_BENCHMARKS": false,
        "UNUSED": "UNUSED"
      }
    }
  ]
}
